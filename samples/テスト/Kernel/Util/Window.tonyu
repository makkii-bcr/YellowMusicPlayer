extends Object;

native _n_setSize;
function setSize(w,h,sz) {
  _n_setSize(w,h,sz);
}
native _n_setPos;
function setPos(x,y) {
  _n_setPos(x,y);
}
native _n_setDrawMode;
function setDrawMode(s) {
  if (s=="A") _n_setDrawMode(1);
  if (s=="B") _n_setDrawMode(2);
  if (s=="C") _n_setDrawMode(3);
}
sizeable=0;